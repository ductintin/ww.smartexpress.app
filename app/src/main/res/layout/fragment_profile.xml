                        <TableRow
                            android:layout_width="match_parent"
                            android:visibility='@{!TextUtils.isEmpty(vm.token) &amp;&amp; !vm.token.equals("NULL") ? View.VISIBLE : View.GONE}'>

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_45sdp"
                                android:background="@drawable/textlines"
                                android:onClick="@{(v) -> vm.doResetPassword()}"
                                android:paddingLeft="@dimen/_15sdp"
                                android:paddingTop="@dimen/_8sdp"
                                android:paddingRight="@dimen/_15sdp"
                                android:paddingBottom="@dimen/_8sdp">

                                <ImageView
                                    android:id="@+id/imageView25"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_icon_password"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                </ImageView>

                                <TextView
                                    style="@style/profile_feature"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/_10sdp"
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/reset_password"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toEndOf="@+id/imageView25"
                                    app:layout_constraintTop_toTopOf="parent">

                                </TextView>

                                <ImageView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_icon_expand"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                </ImageView>
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:visibility='@{!TextUtils.isEmpty(vm.token) &amp;&amp; !vm.token.equals("NULL") ? View.VISIBLE : View.GONE}'>

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_45sdp"
                                android:onClick="@{(v) -> vm.doLogout()}"
                                android:paddingLeft="@dimen/_15sdp"
                                android:paddingTop="@dimen/_8sdp"
                                android:paddingRight="@dimen/_15sdp"
                                android:paddingBottom="@dimen/_8sdp">

                                <ImageView
                                    android:id="@+id/imageView26"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_icon_login"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                </ImageView>

                                <TextView
                                    style="@style/profile_feature"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/_10sdp"
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/logout_all_win"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toEndOf="@+id/imageView26"
                                    app:layout_constraintTop_toTopOf="parent">

                                </TextView>

                                <ImageView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"

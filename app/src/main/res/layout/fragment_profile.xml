                        android:layout_margin="@dimen/_12sdp">

                    </TextView>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingHorizontal="@dimen/_12sdp"
                        android:paddingVertical="@dimen/_15sdp">

                        <TextView
                            style="@style/app_text_b"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginHorizontal="@dimen/_15sdp"
                            android:layout_weight="1"
                            android:maxLines="1"
                            android:text="Xác thực vân tay cho mật khẩu" />

                        <com.kyleduo.switchbutton.SwitchButton
                            android:id="@+id/switchState"
                            android:layout_gravity="right|center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:kswThumbRangeRatio="2.2"
                            android:checked="@{vm.profile.isBiometric ? Boolean.TRUE : Boolean.FALSE}"
                            app:kswThumbDrawable="@drawable/thumb_off"
                            app:kswBackColor="@color/switch_track"
                            app:kswThumbColor="@color/switch_thumb"
                            app:kswThumbHeight="@dimen/_15sdp"
                            app:kswThumbWidth="@dimen/_15sdp">

                        </com.kyleduo.switchbutton.SwitchButton>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@color/white"
                    android:layout_marginTop="@dimen/_10sdp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="CÀI ĐẶT"
                        style="@style/app_text_b"
                        android:textColor="@color/gray"
                        android:layout_margin="@dimen/_12sdp">

                    </TextView>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="@{(v)->vm.doResetPassword()}"
                        android:orientation="horizontal"
                        android:paddingHorizontal="@dimen/_12sdp"
                        android:paddingVertical="@dimen/_15sdp">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_icon_pw" />

                        <TextView
                            style="@style/app_text_b"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginHorizontal="@dimen/_15sdp"
                            android:layout_weight="1"
                            android:maxLines="1"
                            android:text="Cài đặt mật khẩu" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_icon_expand" />

                    </LinearLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:background="@color/bg_color"
                        android:layout_marginLeft="@dimen/_50sdp"
                        android:layout_marginRight="@dimen/_12sdp">

                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingVertical="@dimen/_15sdp"
                        android:paddingLeft="@dimen/_12sdp"
                        android:onClick="@{()->vm.doLogout()}">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_icon_log_out" />

                        <TextView
                            style="@style/app_text_b"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginHorizontal="@dimen/_15sdp"
                            android:layout_weight="1"
                            android:maxLines="1"
                            android:text="@string/logout_all_win" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>


    </LinearLayout>
</layout>